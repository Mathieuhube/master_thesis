    "\n",
    "# Preflop strategy classification with Chatgpt after a discussion about GTO\n",
    "strategy_4bet = {'AA', 'KK', 'AKs', 'AKo'}\n",
    "strategy_3bet = {'AQs', 'QQ', 'JJ', 'TT'}\n",
    "strategy_call = {\n",
    "    'AJs', 'ATs', 'A9s', 'A8s', 'A7s', 'A6s', 'A5s', 'A4s', 'A3s', 'A2s',\n",
    "    'AQo', 'AJo', 'ATo', 'KQo', 'KJo', 'QJo',\n",
